@model wcf_Ticket_Servis_MVC.ViewModel.PersonelViewModel


<div class="container">

    @using (Html.BeginForm("KayitOl", "Login", FormMethod.Post))
    {


        <h2>Kayıt Ol </h2>
        <br /><br />
        <div class="form-group">
            @Html.LabelFor(model => model.Personel.Ad)
            <br />
            @Html.TextBoxFor(model => model.Personel.Ad)
            <br />

        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Personel.Soyad)
            <br />
            @Html.TextBoxFor(model => model.Personel.Soyad)
            <br />

        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Personel.Email)
            <br />
            @Html.TextBoxFor(model => model.Personel.Email)
            <br />

        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Personel.Telefon)
            <br />
            @Html.TextBoxFor(model => model.Personel.Telefon)
            <br />

        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Personel.Password)
            <br />
            @Html.PasswordFor(model => model.Personel.Password)
            <br />

        </div>

        <input type="submit" value="Kayıt Ol" />


    }

</div>
